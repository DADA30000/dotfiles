{
  "log": {
    "level": "debug"
  },
  "inbounds": [
    {
      "listen": "127.0.0.1",
      "listen_port": 2080,
      "tag": "mixed-in",
      "type": "mixed"
    },
    {
      "type": "tun",
      "tag": "tun-in",
      "sniff": true,
      "auto_route": true,
      "strict_route": true,
      "interface_name": "tun0",
      "address": "172.19.0.1/30",
      "route_exclude_address": [
        "1.1.1.1/32",
        "127.0.0.1/32"
      ],
      "stack": "system"
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "endpoints": [
    {
      "system": true,
      "name": "wg0",
      "address": "10.8.1.5/32",
      "private_key": "AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA",
      "type": "wireguard",
      "tag": "proxy",
      "peers": [
        {
          "address": "AAA.AAA.AAA.AAA",
          "port": 99999,
          "public_key": "AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA",
          "pre_shared_key": "AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA",
          "allowed_ips": [
            "0.0.0.0/0",
            "::/0"
          ]
        }
      ]
    }
  ],
  "route": {
    "final": "direct",
    "auto_detect_interface": true,
    "rules": [
      {
        "inbound": [ "mixed-in" ],
        "outbound": "proxy"
      },
      {
        "source_ip_cidr": [ "10.200.0.0/24" ], 
        "outbound": "proxy"
      },
      {
        "process_path_regex": [
          "Battle.net.exe", ".AyuGram-wrapped", ".Discord-wrapped", ".DiscordCanary-wrapped", "TeamSpeak"
        ],
