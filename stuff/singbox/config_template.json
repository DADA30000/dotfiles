{
  "log": {
    "level": "debug"
  },
  "inbounds": [
    {
      "listen": "127.0.0.1",
      "listen_port": 2080,
      "tag": "mixed-in",
      "type": "mixed"
    },
    {
      "type": "tun",
      "tag": "tun-in",
      "sniff": true,
      "auto_route": true,
      "strict_route": true,
      "interface_name": "tun0",
      "address": "172.19.0.1/30",
      "route_exclude_address": [
        "1.1.1.1/32",
        "127.0.0.1/32",
        "192.168.0.0/16"
      ],
      "stack": "system"
    }
  ],
  "outbounds": [
    {
        "type": "selector",
        "tag": "zen-toggle",
        "outbounds": [ "direct", "proxy" ]
    },
    {
      "tag": "proxy2",
      "type": "vless",
      "server": "",
      "server_port": 0,
      "flow": "xtls-rprx-vision",
      "uuid": "",
      "tls": {
        "enabled": true,
        "server_name": "yahoo.com",
        "reality": {
          "enabled": true,
          "public_key": "",
          "short_id": ""
        },
        "utls": {
          "enabled": true,
          "fingerprint": "chrome"
        }
      }
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "endpoints": [
    {
      "system": true,
      "name": "wg0",
      "address": "10.8.1.18/32",
      "private_key": "",
      "type": "wireguard",
      "tag": "proxy",
      "peers": [
        {
          "address": "",
          "port": 0,
          "public_key": "",
          "pre_shared_key": "",
          "allowed_ips": [
            "0.0.0.0/0",
            "::/0"
          ]
        }
      ]
    }
  ],
  "route": {
    "final": "direct",
    "auto_detect_interface": true,
    "rules": [
      {
        "ip_cidr": [
          "127.0.0.1/32",
          "192.168.0.0/16"
        ],
        "outbound": "direct"
      },
      {
        "source_ip_cidr": [
          "127.0.0.1/32",
          "192.168.0.0/16"
        ],
        "outbound": "direct"
      },
      {
        "inbound": [ "mixed-in" ],
        "outbound": "proxy"
      },
      {
        "source_ip_cidr": [ "10.200.0.0/24" ], 
        "outbound": "proxy"
      },
      {
        "process_name": [
          "Battle.net.exe", ".AyuGram-wrapped", ".Discord-wrapped", ".DiscordCanary-wrapped", "TeamSpeak", "Titanfall2.exe", "electron"
        ],
        "outbound": "proxy"
      },
      {
