{
  "manifest_version": 3,
  "name": "Zen sing-box Toggle",
  "version": "1.0",
  "description": "Toggles sing-box VPN status.",
  "permissions": [],
  "host_permissions": [
    "http://127.0.0.1:9090/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "action": {
    "default_title": "Zen Proxy Toggle",
    "default_icon": "icon-direct.svg"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "zen-toggle@nixos.org"
    }
  }
}
