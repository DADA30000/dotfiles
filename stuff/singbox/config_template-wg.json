{
  "log": {
    "level": "debug"
  },
  "dns": {
    "servers": [
      {
        "type": "quic",
        "server": "yournextdns.dns.nextdns.io",
        "tag": "nextdns",
        "domain_resolver": "cloudflare"
      },
      {
        "type": "tcp",
        "tag": "cloudflare",
        "server": "1.1.1.1"
      }
    ],
    "rules": [
      {
        "inbound": "any",
        "outbound": "any",
        "server": "nextdns"
      }
    ]
  },
  "inbounds": [
    {
      "domain_strategy": "",
      "listen": "127.0.0.1",
      "listen_port": 2080,
      "tag": "mixed-in",
      "type": "mixed"
    },
    {
      "type": "tun",
      "tag": "tun-in",
      "auto_route": true,
      "interface_name": "tun0",
      "address": "172.19.0.1/28",
      "mtu": 28160,
      "stack": "system"
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "endpoints": [
    {
      "system": true,
      "name": "wg0",
      "address": "10.8.1.5/32",
      "private_key": "",
      "type": "wireguard",
      "tag": "proxy",
      "peers": [
        {
          "address": "",
          "port": 42800,
          "public_key": "",
          "pre_shared_key": "",
          "allowed_ips": [
            "0.0.0.0/0",
            "::/0"
          ]
        }
      ]
    }
  ],
  "route": {
    "final": "direct",
    "auto_detect_interface": true,
    "rules": [
      {
        "action": "sniff"
      },
      {
        "inbound": [
          "mixed-in"
        ],
        "outbound": "proxy"
      },
      {
        "process_path_regex": [
          "^.*(wget|git|electron|node)$"
        ],
        "outbound": "proxy"
      },
      {
        "domain_suffix": [
          "dis.gd",
          "discord.co",
          "discord.com",
          "discord.design",
          "discord.dev",
          "discord.gg",
          "discord.gift",
          "discord.gifts",
          "discord.media",
          "discord.new",
          "discord.store",
          "discord.tools",
          "discordapp.com",
          "discordapp.net",
          "discordmerch.com",
          "discordpartygames.com",
          "discord-activities.com",
          "discordactivities.com",
          "discordsays.com",
          "discordstatus.com",
          "googlevideo.com",
          "youtu.be",
          "youtube.com",
          "ytimg.com",
          "ggpht.com",
          "animego.org",
          "animego.one",
          "animego.bz",
          "aniboom.one",
          "ya-ligh.com"
        ],
        "outbound": "proxy"
      },
      {
        "process_path_regex": [
          "^.*(.Discord-wrapped)$"
        ],
        "outbound": "proxy"
      },
      {
        "protocol": "dns",
        "action": "hijack-dns"
      }
    ]
  }
}

